import math
def calculate_eigenvalues(matrix: list[list[float|int]]) -> list[float]:

	det=matrix[0][0]*matrix[1][1]-matrix[0][1]*matrix[1][0]
	trace=matrix[0][0]+matrix[1][1]

	value1=(trace+math.sqrt(trace*trace-4*det))/2
	value2=(trace-math.sqrt(trace*trace-4*det))/2

	eigenvalues=[value1,value2]
	return eigenvalues
